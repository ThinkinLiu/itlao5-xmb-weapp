/*----------闪-----------*/

.fade {
  -webkit-animation: fade 2s linear infinite alternate;
  -moz-animation: fade 2s linear infinite alternate;
  -ms-animation: fade 2s linear infinite alternate;
  animation: fade 2s linear infinite alternate;
}

/*----------旋转-----------*/

.Round {
  -webkit-animation: Round 2s infinite ease-in-out;
  -moz-animation: Round 2s infinite ease-in-out;
  -ms-animation: Round 2s infinite ease-in-out;
  animation: Round 2s infinite ease-in-out;
}

/*----------无间隙旋转-----------*/

.rotate {
  -webkit-animation: rotate 2s linear infinite;
  -o-animation: rotate 2s linear infinite;
  -moz-animation: rotate 2s linear infinite;
  -ms-animation: rotate 2s linear infinite;
  animation: rotate 2s linear infinite;
}

/*无间隙旋转*/

@keyframes rotate {
  0% {
    transform: rotate(360deg);
  }

  100% {
    transform: rotate(0deg);
  }
}

/*旋转*/

@keyframes Round {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }

  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}

/*闪*/

@keyframes fade {
  from {
    opacity: 1.0;
  }

  60% {
    opacity: 0.6;
  }

  50% {
    opacity: 0.5;
  }

  40% {
    opacity: 0.4;
  }

  30% {
    opacity: 0.3;
  }

  20% {
    opacity: 0.2;
  }

  10% {
    opacity: 0.1;
  }

  0% {
    opacity: 0;
  }

  10% {
    opacity: 0.1;
  }

  20% {
    opacity: 0.2;
  }

  30% {
    opacity: 0.3;
  }

  40% {
    opacity: 0.4;
  }

  50% {
    opacity: 0.5;
  }

  60% {
    opacity: 0.6;
  }

  to {
    opacity: 1.0;
  }
}

